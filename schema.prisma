generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}
 
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
model todo {
  id            Int         @id @default(autoincrement())
  title         String
  description   String
  dueDate       DateTime
  completed     Boolean     @default(false)
  createdAt     DateTime    @default(now())
}